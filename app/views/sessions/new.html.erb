<h1>Sign in</h1>
<%= form_for :signin, method: "POST" do |f| %>
  <p>
    <%= f.label :name, "Name" %><br />
    <%= f.text_field :name %>
  </p>
  <!--
  <p>
    <%= f.label :email, "Email" %><br />
    <%= f.text_field :email %>
  </p>-->
  <p>
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </p>

  <%= f.submit "Sign in" %>
<% end %>
